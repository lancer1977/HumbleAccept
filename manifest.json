{
    "manifest_version": 2,
    "name": "AcceptEula",
    "description": "Accepts the Eula and Continues",
    "version": "1",
    "author": "me",

    "page_action": {
      "default_popup": "options.html"
    },
    "content_scripts": [
      {
        "matches": ["https://store.steampowered.com/account/*"],
        "js": ["jquery-3.3.1.js","content.js"],
        "css": ["main.css"]
      }
    ],

    "permissions": ["storage","activeTab","declarativeContent"]
}
